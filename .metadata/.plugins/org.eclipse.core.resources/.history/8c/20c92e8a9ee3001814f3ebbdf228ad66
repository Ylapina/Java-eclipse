
public class Person {

	private String FirstName;
	private String LastName;
	private double HourlyRate;
	/**
	 * 
	 */
	public Person() {
		this.FirstName = "John";
		this.LastName = "Doe";
		this.HourlyRate = 10; 
	}
	/**
	 * @param firstName
	 * @param lastName
	 * @param hourlyRate
	 */
	public Person(String firstName, String lastName, double hourlyRate) {
		FirstName = firstName;
		LastName = lastName;
		HourlyRate = hourlyRate;
	}
}
