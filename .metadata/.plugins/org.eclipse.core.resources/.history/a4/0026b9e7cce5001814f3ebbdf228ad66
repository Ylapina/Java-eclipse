		//2nd example - compound assignment
		int a = 100;
		a+= 50; // result should be 150
		System.out.println(a);
		
		int i = 1; //1
		System.out.println(i);
		i = i +1;//2
		System.out.println(i);
		i++;//3
		System.out.println(i);
		i +=2;//5
		System.out.println(i);
		
		
		
		
	
		
		
		
		

	}

